		<Version>3.0.0</Version>
		<Description>让 .NET 开发更简单，更通用，更流行。</Description>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="SpecificationDocument\Assets\index-mini-profiler.html" />
