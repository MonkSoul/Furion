{
  "$schema": "http://json.schemastore.org/template",
  "author": "百小僧",
  "classifications": [
    "Furion",
    "WebApi",
    "Api",
    "RESTful",
    "Swagger",
    "SqlSugar"
  ],
  "name": "Furion Api (SqlSugar)",
  "identity": "Furion.SqlSugar.Template.Api",
  "shortName": "fsapi",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "FurionApi",
  "preferNameDirectory": true,
  "symbols": {
    "framework": {
      "type": "parameter",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net8",
          "description": ".NET8 脚手架"
        },
        {
          "choice": "net9",
          "description": ".NET9 脚手架"
        }
      ],
      "defaultValue": "net8",
      "description": "选择要创建的 .NET8-9 脚手架"
    },
    "net8": {
      "type": "computed",
      "value": "(framework == \"net8\")"
    },
    "net9": {
      "type": "computed",
      "value": "(framework == \"net9\")"
    }
  }
}
