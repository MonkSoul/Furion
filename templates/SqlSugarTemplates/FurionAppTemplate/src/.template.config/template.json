{
  "$schema": "http://json.schemastore.org/template",
  "author": "百小僧",
  "classifications": [
    "Furion",
    "Mvc",
    "WebApi",
    "Api",
    "App",
    "RESTful",
    "Swagger",
    "SqlSugar"
  ],
  "name": "Furion App (SqlSugar)",
  "identity": "Furion.SqlSugar.Template.App",
  "shortName": "fsapp",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "FurionApp",
  "preferNameDirectory": true,
  "symbols": {
    "framework": {
      "type": "parameter",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net6",
          "description": ".NET6 脚手架"
        },
        {
          "choice": "net7",
          "description": ".NET7 脚手架"
        },
        {
          "choice": "net8",
          "description": ".NET8 脚手架"
        },
        {
          "choice": "net9",
          "description": ".NET9 脚手架"
        }
      ],
      "defaultValue": "net8",
      "description": "选择要创建的 .NET6-9 脚手架"
    },
    "net6": {
      "type": "computed",
      "value": "(framework == \"net6\")"
    },
    "net7": {
      "type": "computed",
      "value": "(framework == \"net7\")"
    },
    "net8": {
      "type": "computed",
      "value": "(framework == \"net8\")"
    },
    "net9": {
      "type": "computed",
      "value": "(framework == \"net9\")"
    }
  }
}
